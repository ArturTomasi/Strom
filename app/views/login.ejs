<html data-ng-app="Strom">
    <head>
        <title>Strom</title>
        <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
        <link rel="stylesheet"  type="text/css" href="/css/default.css">
        <link rel="stylesheet"  type="text/css" href="/vendor/bootstrap/dist/css/bootstrap.min.css">
        <link rel="stylesheet"  type="text/css" href="/vendor/bootstrap3-dialog/dist/css/bootstrap-dialog.min.css">
        <link rel="stylesheet"  type="text/css" href="/vendor/components-font-awesome/css/font-awesome.min.css">
    </head>
    <body>
		<div class="container" style="width: 500px; margin: auto">
		    <form class="form-signin" method="post"  action="/login">
		        <h2 class="form-signin-heading">Strom</h2>
		        <label for="inputLogin" class="sr-only">Login</label>
		        <input data-ng-required="true" data-ng-model="username" name="username" type="text" id="inputLogin" class="form-control" placeholder="Login" autofocus>
		        <label for="inputPassword" class="sr-only">Senha</label>
		        <input data-ng-required="true" data-ng-model="password" name="password" type="password" id="inputPassword" class="form-control" placeholder="Senha">

		        <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
		    </form>
		</div>
    </body>
    <script type="text/javascript" src="/vendor/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="/vendor/bootstrap/dist/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/vendor/angular/angular.min.js"></script>
    <script type="text/javascript" src="/vendor/angular-route/angular-route.min.js"></script>
    <script type="text/javascript" src="/vendor/angular-resource/angular-resource.min.js"></script>
    <script type="text/javascript" src="/vendor/angularUtils-pagination/dirPagination.js"></script>
    <script type="text/javascript" src="/vendor/bootstrap3-dialog/dist/js/bootstrap-dialog.min.js"></script>
    <script type="text/javascript" src="/js/Message.js"></script>
    <script type="text/javascript" src="/js/application.js"></script>
    <script type="text/javascript" src="/js/controllers/UserController.js"></script>
    <script type="text/javascript" src="/js/services/UserService.js"></script>
    <script type="text/javascript" src="/js/services/InterceptorService.js"></script>
    
</html>